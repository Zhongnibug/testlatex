@article{zhuanbianguan,
	title = {转变观念:走出人工智能威胁论的误区},
	volume = {39},
	issn = {1672-3910},
	shorttitle = {转变观念},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=4&DbCode=CJFD&dbname=CJFDAUTODAY&filename=LGXB202101001&urlid=&yx=},
	abstract = {对人工智能威胁论的担忧集中体现于人类劳动、理性和控制技术的能力三个方面,其中有两大误区:一是劳动和理性并非人类永恒的特征,二是控制技术本身就是人类的幻想。误区之所以产生,是因为人类对劳动和理性以及人类中心主义的旧观念在作祟。为了更好地促进人工智能发展,就应当及时转变观念,走出威胁论的误区,树立与人工智能"共生"的新观念,同时加速推进"人机共生时代"的社会管理和治理制度的建设。},
	language = {中文;},
	number = {01},
	urldate = {2021-01-28},
	journal = {河南科技大学学报(社会科学版)},
	author = {胡, 良沛},
	year = {2021},
	keywords = {科学理性, 人工智能, 人机共生, 威胁论, AI, human-computer symbiosis, scientific rationality, the threat theory},
	pages = {1--8},
}

@article{jinrongke,
	title = {金融科技背景下金融学人才培养模式的挑战与优化},
	issn = {1004-9487},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=5&DbCode=CJFD&dbname=CJFDAUTODAY&filename=JRJX202101021&urlid=&yx=},
	abstract = {近年来,以新一代信息技术和大数据分析应用为主要核心的金融科技在全世界得以快速发展,其对我国金融行业产生了重要的影响,并且对高校金融学的人才培养和课程内容提出了新的挑战与要求。在传统经济学框架下的高校金融学本科专业人才培养模式和课程体系已不能适应新金融和新时代的需求,在金融科技发展的背景下,我国高校金融学人才培养和课程体系的优化应满足和适应金融科技等新需求,在人才培养目标、金融科技类课程融合、师资队伍建设和课程教学方式方法方面优化完善,从而适应新时代特征、满足新金融需要。},
	language = {中文;},
	number = {01},
	urldate = {2021-01-28},
	journal = {金融理论与教学},
	author = {周, 方召 and 付, 辉 and 贺, 志芳 and 赵, 汝为},
	year = {2021},
	keywords = {金融科技, 金融学专业, 课程优化, 人才培养},
	pages = {94--98},
}

@article{fengxian,
	title = {人工智能在金融市场应用优势、风险及监管研究},
	issn = {1004-9487},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=6&DbCode=CJFD&dbname=CJFDAUTODAY&filename=JRJX202101003&urlid=&yx=},
	abstract = {人工智能被认为是下一个时代的科技制高点。作为一门新兴技术,人工智能具有强大的计算能力和反欺诈技术,实现了金融与技术的深度融合,显著提高了金融服务效率、精准度和系统安全性,为金融创新提供了平台和契机。但人工智能是一把"双刃剑",随着相关领域立法待完善,历史数据未电子化,监管力度不够,研究成本高等弊端导致人工智能的风险隐患日益显露。研究简要描述了人工智能的内涵和发展趋势,分析了它在金融领域的相关应用,总结了它对金融业产生的微观与宏观积极效应。同时,针对人工智能发展过程中存在的风险隐患及实践挑战,基于我国金融发展现实提出促进人工智能在金融市场深入、健康、可持续发展的相关建议。},
	language = {中文;},
	number = {01},
	urldate = {2021-01-28},
	journal = {金融理论与教学},
	author = {夏, 诗园},
	year = {2021},
	keywords = {监管, 金融风险, 金融科技},
	pages = {8--13},
}

@article{fazhannian,
	title = {人工智能技术的发展及其在年龄相关性黄斑变性中的应用},
	issn = {1006-2084},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=7&DbCode=CAPJ&dbname=CAPJLAST&filename=YXZS2021012700L&urlid=11.3553.R.20210127.1048.042&yx=Y},
	abstract = {近年来，基于深度学习的人工智能技术得到了迅速的发展，在医学界引起了广泛的关注。人工智能技术在眼科领域的应用也取得了重大进展。眼科是以影像学为主的学科，如眼底照相、光学相干断层扫描等均适合进行机器学习。随着人口老龄化的加剧，年龄相关性眼病日益普遍，尤其是年龄相关性黄斑变性(AMD)患病率的升高，给医疗保健系统带来了沉重的负担。新技术的产生将有效解决这一难题，人工智能技术可用于AMD预测、检查、诊断、治疗和预后等方面，具有良好的临床应用前景。},
	language = {中文},
	number = {04},
	urldate = {2021-01-28},
	journal = {医学综述},
	author = {操, 文莉 and 陈, 震 and 邢, 怡桥},
	year = {2021},
	keywords = {机器学习, 年龄相关性黄斑变性, 人工智能, 深度学习, Age-related macular degeneration, Artificial intelligence, Deep learning, Machine learning},
	pages = {738--742+747},
}

@article{shehui,
	title = {社会治理智能化：实践创新与路径优化},
	issn = {1672-7223},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=8&DbCode=CAPJ&dbname=CAPJLAST&filename=DZZW20210127004&urlid=11.5181.TP.20210127.1542.008&yx=Y},
	abstract = {提高社会治理智能化水平是完善社会治理体系、推进国家治理体系和治理能力现代化的重要举措。通过智能技术融合社会治理行为、扩充社会治理主体、提升社会治理能力以及增强社会治理效果，社会治理智能化建设实践取得了一系列创新性成果，但仍然存在智能技术与社会治理融合不充分、顶层设计与重心下移不充分以及技术保障与法治改革不充分等问题。作为一项贴近中国基层实践、面向未来的事业，推进社会治理智能化建设，需要始终坚持从实际出发，尊重客观规律，正确处理好顶层设计与基层实施的关系，同时要在法治轨道内深化社会治理智能化建设。},
	language = {中文},
	urldate = {2021-01-28},
	journal = {电子政务},
	author = {刘, 灿华},
	keywords = {大数据, 法治轨道, 人工智能, 社会治理},
	pages = {1--12},
}

@article{weilai,
	title = {面向未来 创新发展——公共图书馆事业高质量发展思考},
	issn = {1002-1167},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=9&DbCode=CAPJ&dbname=CAPJLAST&filename=TSGL20210127001&urlid=44.1306.G2.20210127.1138.004&yx=Y},
	abstract = {中国特色社会主义进入了新时代，推动高质量发展，是适应我国全面建成小康社会、全面建设社会主义现代化国家的必然要求，更是遵循经济规律发展的必然要求。公共图书馆事业经过近20年的高速发展，必须转型适应社会高质量发展，我们必须在新时代中充分考虑社会转型、技术创新和群众需求正在加速促进公共图书馆的转型发展，思考公共图书馆事业未来的发展，这是新时代必须思考的课题。文章在“十四五”发展开启之年，从“十二五”至“十三五”发展时期转型中的公共图书馆事业发展探索实践，未来公共图书馆事业面临的机遇与挑战，以及公共图书馆事业发展的逻辑，思考未来公共图书馆高质量发展的实现路径。},
	language = {中文},
	urldate = {2021-01-28},
	journal = {图书馆论坛},
	author = {王, 惠君},
	keywords = {第三代图书馆, 高质量发展, 公共图书馆, 融合发展, 体系化建设, 智慧图书馆, high-quality development, integrated development, library, smart library, system development, third generation library},
	pages = {1--9},
}

@article{rengong,
	title = {{人工智能产业创新STUIC模型及机理研究}},
	volume = {36},
	issn = {1672-934X},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=10&DbCode=CJFD&dbname=CJFDAUTO&filename=CSSC202101005&urlid=&yx=},
	abstract = {推动人工智能(AI)产业创新有助于回应其发展过程中的不确定性,促进其持续健康发展。AI产业创新STUIC模型由科学、技术、用户、投资、竞争五个核心要素构成,这五个核心要素相互作用、相互影响。AI产业创新STUIC模型内在机理主要表现为联动触发效应、集群扩散效应、优劣析出效应。AI产业创新需要沿着从基础研究到应用研究的技术路线,并注重其内生动力和外在牵引的触发。AI产业集群发展有助于提高其产业竞争力,AI产业的创新扩散则可以为AI产业拓展发展空间。把竞争机制引入AI产业发展中,可以促使其产业优化组合,进而提高AI产业的创新能力。},
	language = {中文;},
	number = {01},
	urldate = {2021-01-28},
	journal = {长沙理工大学学报(社会科学版)},
	author = {宋, 保林},
	year = {2021},
	keywords = {产业创新, 集群扩散效应, 联动触发效应, 人工智能, 优劣析出效应, AI, cluster diffusion effect, industrial innovation, linkage trigger effect, separation effect of advantages and disadvantages, STUIC model, STUIC模型},
	pages = {40--45+53},
}

@article{zidongjia,
	title = {自动驾驶汽车侵权责任的主体识别及担责形式},
	issn = {1004-972X},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=11&DbCode=CJFD&dbname=CJFDAUTO&filename=JJWT202102006&urlid=&yx=},
	abstract = {自动驾驶汽车在遇到交通事故侵权责任的认定时,关于其责任承担的主体及形式问题,学界对此争论已久。在比较域外的立法尝试,探索典型学说的适用可能性之后,既要观察到自动驾驶汽车主体说的四个原则性面向,也要认识到当前完全赋予其独立人格的条件尚不具备。同时,应当将自动驾驶汽车的制造商界定为侵权责任的优势风险承担者,以实现受害人的损害救济、风险的合理化分配以及社会成本最小化。由于自动驾驶系统的不可预见性及超越人类驾驶操控的能力将导致产品缺陷认定的模糊,因此有必要从产品缺陷的判断标准和传统困境出发,证成并建构制造商企业责任"共力模式"的现实路径,合理借鉴"风险效用测试"判断制造商的免责条件,并通过保险和基金制...},
	language = {中文;},
	number = {02},
	urldate = {2021-01-28},
	journal = {经济问题},
	author = {周, 文康 and 胡, 金文 and 张, 丹},
	year = {2021},
	keywords = {保险制度, 产品责任, 交通事故, 制造商企业责任, 自动驾驶汽车, insurance system, manufacturer enterprise responsibility, product liability, self-driving automobiles, traffic accident},
	pages = {37--44},
}

@article{jiyujiqi,
	title = {基于机器学习的矿物智能识别方法研究进展与展望},
	issn = {1000-2383},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=12&DbCode=CAPJ&dbname=CAPJLAST&filename=DQKX20210125003&urlid=42.1874.p.20210126.1450.002&yx=Y},
	abstract = {矿物智能识别是地球科学与信息科学的重要交叉方向，显示出强大的生命力。本文在调研国内外研究动态的基础上，把矿物智能识别划分为四个阶段，即矿物采集、数据获取、模型构建、分类判别；根据测试方法和获得的数据类型，把矿物智能识别分为基于化学成分、显微光学图片、光谱分析的三条基本路线；总结了应用于矿物智能识别的机器学习方法和技术，包括统计学习、线性回归模型、距离度量模型、树结构模型、神经网络模型及其与样本问题相关的新技术。在此基础上，提出消除地质学与人工智能的鸿沟、建设可用于学习的高质量矿物数据集、完善适合矿物智能识别的机器学习方法、增进模型可解释性、加强工业推广的实践是该领域未来的重点发展方向。},
	language = {中文},
	urldate = {2021-01-28},
	journal = {地球科学},
	author = {郝, 慧珍 and 顾, 庆 and 胡, 修棉},
	keywords = {地质学, 机器学习, 矿物学, 矿物智能识别, 人工智能, Artificial intelligence, Geology, Machine Learning, Mineral identification, Mineralogy},
	pages = {1--22},
}

@article{shenduxue,
	title = {深度学习的典型目标检测算法研究综述},
	issn = {1002-8331},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=13&DbCode=CAPJ&dbname=CAPJLAST&filename=JSGG20210125002&urlid=11.2127.TP.20210126.1445.006&yx=Y},
	abstract = {目标检测是计算机视觉的一个重要研究方向，其目的是精确识别给定图像中特定目标物体的类别和位置。近年来，深度卷积神经网络（DeepConvolutional Neural Networks，DCNN）所具有的特征学习和迁移学习能力，在目标检测算法特征提取、图像表达、分类与识别等方面取得了显著进展。文章介绍了基于深度学习目标检测算法的研究进展、常用数据集特点以及性能指标评价的关键参数，对比分析了双阶段、单阶段以及其他改进算法的网络结构和实现方式。最后阐述了算法在人脸、显著目标、行人、遥感图像、医学图像、粮虫等检测领域的应用进展，结合当前存在的问题和挑战，展望分析了其未来的研究方向。},
	language = {中文},
	urldate = {2021-01-28},
	journal = {计算机工程与应用},
	author = {许, 德刚 and 王, 露 and 李, 凡},
	keywords = {计算机视觉, 目标检测, 迁移学习, 深度学习, 特征提取, computer vision, deep learning, feature extraction, object detection, transfer learning},
	pages = {1--21},
}

@article{jiyuhe,
