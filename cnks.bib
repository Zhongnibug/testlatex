
@article{rengongzhi,
	title = {人工智能背景下高职院校会计核算发展思路探讨},
	issn = {1671-6728},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=1&DbCode=CJFD&dbname=CJFDAUTODAY&filename=SYJW202104023&urlid=&yx=},
	abstract = {随着社会的不断发展,电子信息技术的日臻成熟,人工智能已经在各个领域中进行有机运用,会计核算也逐步由实物资源向信息资源进行转变。现阶段,人工智能的全面应用已经被列入高职院校改革的重点问题。本文结合高职院校发展实际,对人工智能背景下高职院校会计核算发展思路做出探讨,以期对需要这类资料的相关人士提供一定帮助,从而实现高职院校会计核算工作的有效开展。},
	language = {中文;},
	number = {04},
	urldate = {2021-01-28},
	journal = {商讯},
	author = {陈, 飞琴},
	year = {2021},
	keywords = {发展思路, 高职院校, 会计核算, 人工智能},
	pages = {45--46},
}

@article{jiyuzhishi,
