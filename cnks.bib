@article{jiyuqianyin,
	title = {基于牵引控制的深度增强学习路由策略生成},
	issn = {1000-1239},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=16&DbCode=CAPJ&dbname=CAPJLAST&filename=JFYZ20210125001&urlid=11.1777.TP.20210126.1024.002&yx=Y},
	abstract = {当前网络规模的高速增长带来网络流量复杂度的日益提高，增加了对流量特征精确建模的难度.近年来业界提出使用深度增强学习技术实现网络路由的智能化生成，一定程度上克服了人工进行流量分析和建模的缺点.然而，目前提出的解决方案普遍存在可扩展性差等问题.对此，提出了一种基于牵引控制理论的深度增强学习路由策略生成技术Hierar-DRL，通过引入牵引控制理论并结合深度增强学习的自动策略搜索能力，提高了智能路由算法可扩展性.仿真实验结果表明，所提方案相比当前最优方案的端到端时延最多降低了28.5\%，证明了所提智能路由方案的有效性.},
	language = {中文},
	urldate = {2021-01-28},
	journal = {计算机研究与发展},
	author = {孙, 鹏浩 and 兰, 巨龙 and 申, 涓 and 胡, 宇翔},
	keywords = {路由优化, 牵引控制, 人工智能, 软件定义网络, 深度增强学习, artificial intelligence, deep reinforcement learning, pinning control, routing optimization, software-defined networking},
	pages = {1--13},
}

@article{jishixing,
	title = {深度学习模型可解释性研究综述},
	issn = {1002-8331},
	url = {https://kns.cnki.net/KNS8/Detail?sfield=fn&QueryID=0&CurRec=17&DbCode=CAPJ&dbname=CAPJLAST&filename=JSGG20210125001&urlid=11.2127.TP.20210126.0948.002&yx=Y},
	abstract = {深度学习技术以数据驱动学习的特点，在自然语言处理、图像处理、语音识别等领域取得了巨大成就。但由于深度学习模型网络过深、参数多、复杂度高等特性，该模型做出的决策及中间过程让人类难以理解，因此探究深度学习的可解释性成为当前人工智能领域研究的新课题。本综述以深度学习模型可解释性为研究对象，对其研究进展进行总结阐述。首先从自解释模型、特定模型解释、不可知模型解释、因果可解释性四个方面对主要可解释性方法进行总结分析。同时列举出可解释性相关技术的应用，最后讨论当前可解释性研究存在的问题并进行展望，以推动深度学习可解释性研究框架的进一步发展。},
	language = {中文},
	urldate = {2021-01-28},
	journal = {计算机工程与应用},
	author = {曾, 春艳 and 严, 康 and 王, 志锋 and 余, 琰 and 纪, 纯妹},
	keywords = {可解释性, 人工智能, 深度学习, 因果可解释, 自解释, artificial intelligence, causal interpretability, deep learning, interpretability, self-explanatory},
	pages = {1--11},
}

